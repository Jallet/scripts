#!/bin/sh
if [ $# -eq 0 ] || [ $# -ge 2 ]
then
    echo "[Usage]: . goto <destination>"
    exit
fi

case $1 in
    ck)
        cd /home/curio/code/colorization-keras
        ;;
    ck1)
        cd /home/curio/code/colorization-keras-1
        ;;
    *)
        echo "Wrong destination"
        exit
        ;;
esac
