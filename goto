#!/bin/sh
if [ $# -eq 0 ] || [ $# -ge 2 ]
then
    echo "[Usage]: . goto <destination>"
    exit
fi

case $1 in
    cc)
        cd /home/liangjiang/code/caffe_colorization
        ;;
    c)
        cd /home/liangjiang/code/caffe
        ;;
    rn)
        cd /home/liangjiang/code/residual_network
        ;;
    *)
        echo "Wrong destination"
        ;;
esac
