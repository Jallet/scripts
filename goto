#!/bin/sh
if [ $# -eq 0 ] || [ $# -ge 2 ]
then
    echo "[Usage]: . goto <destination>"
    exit
fi

case $1 in
    ahk)
        cd /d/software/AHK_scripts
        ;;
    liang)
        cd /c/Users/Liang
        ;;
    software)
        cd /d/software
        ;;
    *)
        echo "Wrong destination"
        ;;
esac
